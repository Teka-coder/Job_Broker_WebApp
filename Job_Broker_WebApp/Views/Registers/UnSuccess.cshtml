@model Job_Broker_WebApp.Models.Register

@{
    ViewBag.Title = "UnSuccess";
    //Layout = "~/Views/Shared/_Layout.cshtml";
    Layout = null;
}

@*<h2>UnSuccess</h2>*@





<!doctype html>
<!--

* Website: https://github.com/alexis-luna/bootstrap-simple-admin-template
-->
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>UnSuccess | Registration failed!</title>
    <link href="~/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/assets/css/error.css" rel="stylesheet" />

</head>

<body>
    <div class="wrapper">
        <div class="page vertical-align text-center">
            <div class="page-content vertical-align-middle">
                <header>
                    <h1 class="animation-slide-top">Registration failed !</h1>
                    <p>ወደ ደቡብ አሰሪ ና ሰራተኛ አገናኝ ሲይስተም ለመመዝገብ ያደረጉት ሙከራ አልተሳካም !</p>
                </header>
                @*<p class="error-advise">@Session["USERNAME"].ToString() is registered succesfully !.</p>*@
                <p class="error-advise" style="color:red">ምዝገባው አልተሳካም !.</p>
                <a class="btn btn-success btn-flat mb-5 col-sm-12">ይህ @Session["USERNAME"].ToString() የተባለ ስም ተይዟል !</a>
                <div class="row">
                    @*<div class="col-xs-4">
                        <button class="btn btn-primary btn-flat mb-5">@Html.ActionLink("Log in", "Login", "Home")</button>
                    </div>*@
                    <div class="col-xs-6">
                        <button class="btn btn-default btn-flat mb-5">@Html.ActionLink("Try again", "Create", "Registers")</button>
                    </div>
                    <div class="col-xs-6">
                        <button class="btn btn-default btn-flat mb-5">@Html.ActionLink("Home", "Index", "Home")</button>
                    </div>

                </div>
                <footer class="page-copyright">
                    <p>© 2021. All RIGHT RESERVED.</p>
                </footer>
            </div>
        </div>
    </div>
    <script src="assets/vendor/jquery/jquery.min.js"></script>
    <script src="assets/vendor/bootstrap/js/bootstrap.min.js"></script>
</body>

</html>



